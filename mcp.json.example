{
  "mcpServers": {
    "mcp-database-server": {
      "command": "node",
      "args": [
        "/path/to/mcp-database-server/dist/index.js",
        "--config",
        "/path/to/.mcp-database-server.config"
      ],
      "env": {
        "DB_URL_POSTGRES": "postgresql://user:password@localhost:5432/dbname",
        "DB_URL_MYSQL": "mysql://user:password@localhost:3306/dbname"
      }
    }
  }
}
