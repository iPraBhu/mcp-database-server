# Database connection URLs
# PostgreSQL
DB_URL_POSTGRES=postgresql://user:password@localhost:5432/dbname

# MySQL
DB_URL_MYSQL=mysql://user:password@localhost:3306/dbname

# SQL Server
DB_URL_MSSQL=Server=localhost,1433;Database=dbname;User Id=sa;Password=yourStrong(!)Password;Encrypt=true;TrustServerCertificate=true

# SQLite (file path)
DB_URL_SQLITE=./data/app.db

# Oracle
DB_URL_ORACLE=user/password@localhost:1521/XEPDB1

# Logging
LOG_LEVEL=info
